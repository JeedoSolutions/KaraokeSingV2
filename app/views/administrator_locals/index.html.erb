<p id="notice"><%= notice %></p>

<h1>Listar Administrador de Local</h1>

<table>
  <thead>
    <tr>
      <th>Trabajador</th>
      <th>Local</th>
      <th>Estado</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administrator_locals.each do |administrator_local| %>
      <tr>
        <td><%= administrator_local.employee %></td>
        <td><%= administrator_local.local %></td>
        <td><%= administrator_local.estado %></td>
        <td><%= link_to 'Mostrar', administrator_local %></td>
        <td><%= link_to 'Editar', edit_administrator_local_path(administrator_local) %></td>
        <td><%= link_to 'Eliminar', administrator_local, method: :delete, data: { confirm: '¿Está Seguro?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Nuevo Administrador de Local', new_administrator_local_path %>
